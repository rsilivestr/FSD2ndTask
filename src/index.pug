- const pages = [{ title: 'Colors', url: 'pages/colors.html' }, { title: 'Form Elements', url: 'pages/form-elements.html' }, {title: 'Cards', url: 'pages/cards.html' }, { title: 'Headers & Footers', url: 'pages/headers-footers.html' }, { title: 'Landing', url: 'pages/landing.html' }, { title: 'Search room', url: 'pages/search-room-filter.html' }, { title: 'Room details', url: 'pages/room-details.html' } ]

html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="X-UA-Compatible", content="ie=edge")
    title Pages
  body.router
    nav.router__nav
      i.icon-menu
      each page in pages
        a.router__navlink(href=page.url)=page.title
